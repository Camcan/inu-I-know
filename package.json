{
  "name": "react-spike",
  "version": "1.0.0",
  "description": "Simple react spike session",
  "main": "server.js",
  "dependencies": {
    "exenv": "^1.2.2",
    "express": "^4.14.0",
    "history": "^2.0.0",
    "react": "^15.3.2",
    "react-binding": "^0.7.5",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.2",
    "react-icons": "^2.2.1",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "css-modulesify": "^0.25.1",
    "red-tape": "0.0.2",
    "tape": "^4.6.0",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0",
            "react"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "start": "forever server.js",
    "watch": "watchify -p [css-modulesify -o public/bundle.css] client/app.js -o public/scripts/bundle.js -dv & nodemon server.js",
    "bundle": "browserify client/app.js -o public/scripts/bundle.js",
    "test": "echo \"Ain't nobody got time for that.\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Camcan/inu-I-know.git"
  },
  "author": "Campbell Hawkes",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Camcan/inu-I-know/issues"
  },
  "homepage": "https://github.com/Camcan/inu-I-know#readme"
}
